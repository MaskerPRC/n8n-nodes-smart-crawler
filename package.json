			"dist/nodes/SmartCrawler/SmartCrawler.node.js"
		]
	},
	"devDependencies": {
		"@n8n/node-cli": "*",
		"@types/jest": "^29.5.12",
		"eslint": "9.32.0",
		"jest": "^29.7.0",
		"prettier": "3.6.2",
		"release-it": "^19.0.4",
		"ts-jest": "^29.1.2",
		"typescript": "5.9.2"
	},
	"peerDependencies": {
		"n8n-workflow": "*"
	},
	"dependencies": {
		"axios": "^1.13.4",
